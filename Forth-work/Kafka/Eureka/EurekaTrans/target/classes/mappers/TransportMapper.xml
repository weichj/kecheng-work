<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.EurekaTrans.Mapper.TransportMapper">

    <insert id="CreateTransport" >
        INSERT INTO `transport` VALUES (#{transId},#{transName})
    </insert>

    <select id="SelectAllTransport" resultType="com.example.EurekaTrans.Bean.Transport">
        SELECT * FROM `transport`
    </select>

</mapper>